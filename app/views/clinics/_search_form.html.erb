<%= search_form_for @q, url: clinics_path do |f| %>
  <div class="input-group mb-3">
    <%= f.search_field :clinic_name_cont, class: 'form-control w-auto', placeholder: "クリニック名" %>
    <div class="input-group-append">
      <%= f.submit "検索", class: 'btn btn-primary' %>
    </div>
  </div>
<% end %>